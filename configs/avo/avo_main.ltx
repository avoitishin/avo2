[mod]
version = 0.4

;[logging] - moved to avo_log
;visual 	= true		; display and log hud messages (vlog)
;error		= true		; log error messages (elog)
;warning	= true		; log warning messages (wlog)
;debug		= true		; log debug messages (dlog)
;printf	= false		; enable GCS print function (note: this will create lots of output)

[debug]
lua_debug	= true		; enable lua debug by RvP (needs bin\lua_debug.dll)
unit_test	= true		; enable unit test harness

; [profiler]
; enabled = false 		; turn on avo profiler (currently not implemented)

[keys]
avo_save = F6				; avo quick hard save
avo_load = F10			; avo quick load of last save