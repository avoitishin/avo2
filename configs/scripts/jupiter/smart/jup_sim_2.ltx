[smart_terrain]
respawn_idle = 21600
respawn_only_level = true
respawn_radius = 200
death_idle_time = 21600

squad_id = 40
max_population = 2 ; 1
respawn_params = respawn@jup_sim_2

[respawn@jup_sim_2]
spawn_bandit
spawn_bandit@elite

spawn_bandit@hostile
spawn_bandit@hostile_elite

spawn_bandit_2
spawn_bandit_2@elite

spawn_bandit_2@hostile
spawn_bandit_2@hostile_elite

