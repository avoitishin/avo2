[fieldcooker]:booster
$spawn 				= "devices\repair_kit_10"
can_trade			= true
quest_item      	= false
visual					= dynamics\cooking\multi_fuel_stove.ogf
cost					= 6055
inv_name				= st_fieldcooker
inv_name_short				= st_fieldcooker
description				= st_fieldcooker_descr
inv_weight				= 0.46
inv_grid_width			= 2
inv_grid_height			= 1
inv_grid_x				= 20
inv_grid_y				= 4

cooking_use_parts	 	= true	;Allows the use of lesser conditioned weapons to be used for parts to add overall bonus to cooking
cooking_type			 	= fuel	;weapon or outfit
cooking_refuse			= charcoal, charcoal2, charcoal3, explo_metalcan_powder ;Add sections separated by commas that cannot be cookinged by this item
cooking_only				=          ;Add sections separated by commas that exclude all sections but these
cooking_parts_include    = mutant_part_tushkano_meat, mutant_part_flesh_meat, mutant_part_boar_chop, mutant_part_dog_meat, mutant_part_psevdodog_meat, mutant_part_krovosos_meat, mutant_part_snork_hand, mutant_part_chimera_meat, mutant_part_crow_beak			;Add sections of items to include to be considered cooking parts
cooking_parts_exclude	= kerosene, kerosene2, kerosene3, kerosene4, kerosene5, explo_jerrycan_fuel, explo_jerrycan_fuel2, explo_jerrycan_fuel3, explo_jerrycan_fuel4, explo_jerrycan_fuel5, explo_jerrycan_fuel6, explo_jerrycan_fuel7, explo_jerrycan_fuel8, explo_balon_gas, explo_balon_gas2, explo_balon_gas3, explo_balon_gas4, explo_balon_gas5, explo_balon_gas6, explo_balon_gas7, explo_balon_gas8
cooking_use_actor_effects = true      ;Uses actor_effects script. Setup a *_dummy section for the item in actor_effects_data.script

;use_sound			= interface\inv_cooking

